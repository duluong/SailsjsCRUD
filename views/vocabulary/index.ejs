<div class="container">
 <div class="jumbotron">
    <h1>Bài học từ vựng</h1> 
 </div>


 <!--<div class="page-header bg-info">
  <h1>Bài học từ vựng</h1>
</div> -->

<a class="btn btn-success btn-xs" href="/vocabulary/create">Add</a>
<table class='table table-bordered table-hover'>
  <tr>
  <th>No</th>
  <th>Từ vựng</th>
  <th></th>
  <th>Ý nghĩa</th>
  <th></th></tr>

<% vocabularies.forEach( function( model, index){ %>
  <tr>
  <td><%= index + 1 %></td>
  <td><%= model.word %></td>
  <td><img src="/images/<%=model.image%>" height="70" width="70"></td>
  <td><%= model.meaning %></td>
  <td>
  <a class="btn btn-info btn-xs" href="/vocabulary/view/<%=model.id %>">Detail</a>
  <a class="btn btn-warning btn-xs" href="/vocabulary/update/<%=model.id %>">Update</a>  
  <a class="btn btn-danger btn-xs" href="/vocabulary/delete/<%=model.id %>">delete</a>
  </td></tr>
<% }); %>
</table>

 </div>
